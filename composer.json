{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rafis/schema-extended"
        }
    ],
    "require": {
        "php": ">=7.0",
        "barryvdh/laravel-ide-helper": "^2.3",
        "barryvdh/laravel-translation-manager": "0.2.x",
        "doctrine/dbal": "^2.5.13",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.3.0",
        "maatwebsite/excel": "^2.1.0",
        "rafis/schema-extended": "~1.0",
        "samkitano/aktiv8me": "1.*",
        "sentry/sentry": "^1.7",
        "sentry/sentry-laravel": "^0.8.0",
        "twbs/bootstrap": "^3.3",
        "wallee/sdk": "^1.1.0",
        "yajra/laravel-datatables-oracle": "7.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "ignasbernotas/laravel-model-generator": "^1.2",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "xethron/migrations-generator": "^2.0",
        "kartik-v/bootstrap-fileinput": "4.4.3",
        "propaganistas/laravel-phone": "^3.0.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
